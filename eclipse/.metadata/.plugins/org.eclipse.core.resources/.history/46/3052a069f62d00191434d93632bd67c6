package net.modificationstation.stationloader.events.client;

import net.minecraft.src.GuiScreen;
import net.modificationstation.stationloader.events.common.Event;

public class MCPreDisplayGuiScreenEvent extends Event{
    public MCPreDisplayGuiScreenEvent(GuiScreen guiscreen) {
        eventData = guiscreen;
    }
    public GuiScreen getGuiScreen() {
        return eventData;
    }
    public void setGuiScreen(GuiScreen guiscreen) {
        eventData = guiscreen;
    }
    private GuiScreen eventData;
}
