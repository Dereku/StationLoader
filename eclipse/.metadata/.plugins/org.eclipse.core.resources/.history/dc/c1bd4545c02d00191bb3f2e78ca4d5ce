package net.modificationstation.stationloader.client.events;

import net.minecraft.src.GuiScreen;
import net.modificationstation.stationloader.events.common.Event;

public class MCPreDisplayGuiScreen extends Event{
    public MCPreDisplayGuiScreen(GuiScreen guiscreen) {
        eventData = guiscreen;
    }
    public GuiScreen getGuiScreen() {
        return eventData;
    }
    public void setGuiScreen(GuiScreen guiscreen) {
        eventData = guiscreen;
    }
    private GuiScreen eventData;
}
