package net.modificationstation.stationloader.events.client;

import net.minecraft.src.GuiScreen;
import net.modificationstation.stationloader.events.common.Event;

public class MCGuiDrawScreenEvent extends Event{

    public MCGuiDrawScreenEvent(GuiScreen guiscreen, int i, int j, float f) {
        eventData = new Object[] {guiscreen, i, j, f};
    }
    public boolean continue() {
        return cancelled;
    }
    public GuiScreen getGuiScreen() {
        return eventData[]
    }
    
    private Object eventData[];
    
}
